<%= javascript_include_tag "datepicker" %>
<%= javascript_include_tag "datepicker.en" %>
<%= javascript_include_tag "datepicker.custom" %>
<!-- Plugins JS Ends-->
<!-- Theme js-->
<%= javascript_include_tag "script" %>
<%= javascript_include_tag "customizer" %>
